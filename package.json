{
  "name": "obsidian-selective-publisher",
  "version": "1.0.0",
  "description": "An Obsidian plugin that selectively publishes notes to a public repository",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint src",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "publishing",
    "git",
    "github-pages"
  ],
  "author": "Ivan Grubišić",
  "license": "MIT",
  "devDependencies": {
    "@electron/remote": "^2.1.3",
    "@types/node": "^25.0.3",
    "@types/picomatch": "^4.0.2",
    "esbuild": "^0.27.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "globals": "^17.0.0",
    "obsidian": "latest",
    "picomatch": "^4.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {}
}
